{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "allowJs": true,
    "typeRoots": [

      "./types",
      
      "./node_modules/vue/types"
      
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": ["./*"],
      "~/*": ["./node_modules/*"]
    },
    "skipLibCheck": true
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true,
    "target": 2.7,
    "extensions": [
      ".vue"
    ]
  },
  "include": ["src"]
}
